{
  "version": 2,
  "name": "sync-battle",
  "builds": [
    { "src": "package.json", "use": "@now/next" },
    { "src": "?(next-server)/+(game|server)/*.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/graphql.*", "dest": "next-server/server/index.js" },
    {
      "src": "/fallback/(.*)",
      "dest": "next-server/server/fallback.js?fallbackUrl=$1"
    }
  ]
}
