{
  "version": 2,
  "name": "sync-battle",
  "builds": [
    { "src": "package.json", "use": "@now/next" },
    { "src": "+(server|game|lib)/*.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/graphql.*", "dest": "server/babel-index.js" },
    {
      "src": "/fallback/(.*)",
      "dest": "server/babel-fallback.js?fallbackUrl=$1"
    }
  ]
}
